{
  "name": "rari-demo",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "A Runtime Accelerated Rendering Infrastructure (Rari) application",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run typecheck && vite build",
