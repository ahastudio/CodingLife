    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript/native-preview": "7.0.0-dev.20250611.1",
    "@vitejs/plugin-react-oxc": "^0.2.3",
    "rolldown-vite": "^7.1.20",
    "tailwindcss": "^4.1.16"
  }
}
