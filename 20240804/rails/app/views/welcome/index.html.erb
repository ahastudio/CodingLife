<form action="<%= custom_upload_file_path %>" method="post" enctype="multipart/form-data">
  <h2>Upload Attachment</h2>
  <p>
    <label for="attachment_file">File:</label>
    <input type="file" name="file" id="attachment_file" />
  </p>
  <p>
    <input type="submit" value="Upload" />
  </p>
</form>
